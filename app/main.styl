*
  box-sizing border-box
body
  font-family "Helvetica Neue", Helvetica, Arial, sans-serif
  font-size 14px
  line-height 1.42857143
  color #333
  background-color #fff
  margin 0
  min-width 414px
.h1, h1
  font-family inherit
  font-weight 500
  line-height 1.1
  color inherit
  margin 0
.h1, h1, button, input
  font-size 21px
.right
  float right
.left
  float left
